{
  "name": "@libchrono/core",
  "version": "1.0.15",
  "description": "Small library to handle durations parsing and displaying.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "author": "Damian Wile≈Ñski",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/damianw27/libchrono.git"
  },
  "keywords": [
    "duration",
    "parser",
    "validator",
    "time",
    "string",
    "parse",
    "validate",
    "math",
    "operations",
    "weeks",
    "days",
    "hours",
    "minutes",
    "seconds",
    "milliseconds"
  ],
  "scripts": {
    "build:grammar": "antlr4ts -o src/generated ./src/antlr/Duration.g4",
    "build:lib": "webpack && tsc-alias -p tsconfig.json",
    "build:types": "tsc --emitDeclarationOnly && tsc-alias -p tsconfig.json",
    "build:all": "yarn build:grammar && yarn build:lib && yarn build:types",
    "test": "jest",
    "coverage": "yarn test --coverage && make-coverage-badge",
    "documentation": "typedoc",
    "deploy": "yarn test && yarn build:all && yarn publish"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.3",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "antlr4ts-cli": "^0.5.0-alpha.4",
    "eslint": "^8.36.0",
    "eslint-config-alloy": "^4.9.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-webpack-plugin": "^4.0.1",
    "jest": "^29.5.0",
    "make-coverage-badge": "^1.2.0",
    "node": "^19.8.1",
    "prettier": "^2.8.4",
    "terser-webpack-plugin": "^5.3.7",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.4",
    "tsconfig-paths-webpack-plugin": "^4.0.1",
    "typedoc": "^0.24.4",
    "typescript": "^5.0.4",
    "webpack": "^5.80.0",
    "webpack-cli": "^5.0.2"
  },
  "dependencies": {
    "antlr4ts": "^0.5.0-alpha.4",
    "assert": "^2.0.0",
    "util": "^0.12.5"
  }
}
